{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/hello.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/hello",
      "dest": "api/hello.ts"
    },
    {
      "src": "/",
      "dest": "index.ts"
    }
  ]
}
